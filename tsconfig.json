{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "noEmit": true,
    "moduleResolution": "node" // Asegúrate de que esta opción esté configurada
  },
  "include": ["src"]
}
